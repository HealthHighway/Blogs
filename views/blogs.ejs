  <body>
    <div class="top-blog-div" style="">
      <nav
        class="navbar navbar-expand-lg navbar-dark"
        id="navbar-landing"
        style="
          position: fixed;
          z-index: 100;
          width: 100vw !important;
          transition: cubic-bezier(0.075, 0.82, 0.165, 1);
        "
      >
        <div
          id="navbar-container"
          class="container-fluid navbar-container-unscrolled"
        >
          <a class="navbar-brand" href="/">
            <img
              src="/images/logo.svg"
              alt="Health Highway Logo"
              class="d-inline-block align-text-center"
            />
            <span style="color: #4ca9ee"
              >Health<span style="color: #29e7cd"> Highway</span></span
            >
          </a>
          <button class="navbar-toggler" type="button" onclick="openNav()">
            <!-- <span class="navbar-toggler-icon"></span> -->
            <i style="color: #4ca9ee" class="fa fa-bars" aria-hidden="true"></i>
          </button>
          <div
            class="collapse navbar-collapse"
            id="navbarNavDropdown"
            style="margin-right: 50px"
          >
            <ul class="navbar-nav ml-auto">
              <button
                id="landing-joinus"
                type="button"
                class="btn btn-light"
                onclick="showmodal('booksession')"
              >
                Join Us
              </button>
            </ul>
          </div>
          <!-- sidebar overlay -->
          <div id="mySidenav" class="sidenav">
            <a href="javascript:void(0)" class="closebtn" onclick="closeNav()"
              >&times;</a
            >
            <button
              id="sidebar-joinus"
              type="button"
              class="btn btn-light"
              onclick="showmodal('booksession')"
            >
              Join Us
            </button>
          </div>
        </div>
      </nav>
    </div>

    <!-- Modal -->
   
    
    <!-- modal for booking session ends here -->

    <div class="recent-blog-div">
      <a href="/blogs/international-yoga-day">
        <div
          id="carouselExampleIndicators"
          class="carousel slide"
          data-ride="carousel"
        >
          <div class="carousel-inner">
            <div class="carousel-item active">
              <img
                class="d-block"
                src="/images/intyoga-3.png"
                alt="First slide"
              />
              <div class="carousel-caption d-md-block">
                <h3>International Yoga Day</h3>
                <p>Jan 21, 2021</p>
              </div>
            </div>
          </div>
        </div>
      </a>
      <div class="all-blogs"> 
            <% blogData.forEach((elemenet,index) => { %>
              <div class="card" style="width: 18rem">
            <img
              src="<%= elemenet.image %>"
              class="card-img-top"
              alt="..."
            />
            <div class="card-body">
              <h5 class="card-title"><%= elemenet.heading %> </h5>
              <p class="card-text">
                <%= elemenet.discription.substring(0,80) + "..." %>  
              </p>
              <a
                href="/blogs/<%= elemenet.heading %>"
                class="btn btn-primary"
                >Read More</a
              >
            </div>
          </div>
              <% }) %>
        </div>
        <button
        type="button"
        class="btn btn-primary"
        onclick="showmodal('booksession')"
      >
        Book a Session
      </button>
      </div>
        
    </div>
  </div>
</div>
