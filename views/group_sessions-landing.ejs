<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.15.3/css/all.css" integrity="sha384-SZXxX4whJ79/gErwcOYf+zWLeJdY/qpuqC4cAa9rOGUstPomtqpuNWT9wdPEn2fk" crossorigin="anonymous" />
    <link rel="stylesheet" href="https://unpkg.com/swiper/swiper-bundle.min.css" />
    <link rel="stylesheet" href="css/main.css">
    <link rel="stylesheet" href="css/new_navbar.css">
    <link rel="stylesheet" href="css/login.css" />
    <link rel="stylesheet" href="css/search-fliter.css">
    <title>Health Highway | Search for Group Sessions</title>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://apis.google.com/js/api:client.js"></script>
</head>

<body>
    <header>
        <%- include("partials/navbar") %>
    </header>
    <main>
        <!-- Search-Filter (Start) -->
        <div class="grp-session-container">

            <div class="tags-container">
                <button id="fitness" class="tag">Fitness</button>
                <button id="Yoga for kids" class="tag">Hypertension</button>
                <button id="Diabetes" class="tag">Diabetes</button>
                <button id="Obesity" class="tag">Obesity</button>
                <button id="Trending" class="tag">Trending</button>

            </div>
            <div class="trending-classes-container">
                <div class="trending-heading">
                    <img src="./images/trending2.svg" alt="" />
                    <h2>Trending Group Sessions</h2>
                </div>
                <div class="trending-classes">
                    <% sessions.forEach(session => { %>
                        <a href="/Group-Yoga-Classes/<%= session._id %>">
                            <div class="grp-card">
                                <img src=<%= session.IMG %> alt="" />
                                <div class="grp-card-content">
                                    <div class="grp-card-details">
                                        <div class="grp-card-title">
                                            <h3><%= session.SESSION_NAME %></h3>
                                            <p><%= session.TIME %></p>
                                        </div>
                                        <h4>by Yogacharya <%= session.TNAME %></h4>
                                    </div>
                                    <div class="grp-card-trust">
                                        <img src="./images/man-vector.svg" alt="" />
                                        <h4>Trusted by <%= session.PARTICIPANTS %>+ people</h4>
                                    </div>
                                </div>
                            </div>
                        </a>
                    <% }) %>
                   
                </div>
            </div>
        </div>
        <!-- Search-Filter (End) -->

        <!-- USP Section (START) -->
        <%- include('partials/usp') %>
        <!-- USP Section (END) -->

        <!-- Testimonials -->

        <%- include('partials/testimonials') %>

        <!-- Swiper JS -->
        <script src="https://unpkg.com/swiper/swiper-bundle.min.js"></script>

        <!-- Initialize Swiper -->
        <script>
            var swiper = new Swiper(".myTestimonialSwiper", {
                spaceBetween: 20,
                // loop:true,
                navigation: {
                    nextEl: ".swiper-button-next",
                    prevEl: ".swiper-button-prev",
                },
            });
        </script>

        <!-- Testimonials(end) -->

        <!-- Call-Us Section  -->
        <%- include('partials/callback') %>
        <!-- Call-Us Section (END) -->

        </div>


    </main>

    <!-- login div -->
    <%- include('partials/login-form') %>
    <!-- login div (END) -->

        <footer></footer>

        <script src="js/search.js"></script>
        <script src="js/landing.js"></script>
        <script type="text/javascript" src="js/login.js"></script>
</body>

</html>